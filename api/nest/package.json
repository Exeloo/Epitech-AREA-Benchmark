{
  "name": "area-api",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Exeloo/B-DEV-500_AREA.git"
  },
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "nest build",
    "lint": "eslint . && prettier --check . && tsc --noemit",
    "fix": "eslint . --fix && prettier --write .",
    "test": "jest",
    "test:unit": "jest --config ./jest.config.json",
    "test:e2e:migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/config/tests/migration.test.config.ts",
    "test:e2e:run": "jest --config ./jest-e2e.config.json --runInBand --detectOpenHandles --forceExit",
    "test:e2e": "docker exec area-api-1 sh -c 'yarn test:e2e:migration && yarn test:e2e:run'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d src/config/migration.config.ts",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/config/migration.config.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/config/migration.config.ts",
    "cli": "node dist/area-cli.js",
    "serve-nest": "nest start --debug --watch",
    "serve": "docker compose -p area -f .cloud/docker/docker-compose.local.yml up -d",
    "docker-up": "docker compose -f .cloud/docker/docker-compose.prod.yml up",
    "docker-build": "docker compose -f .cloud/docker/docker-compose.prod.yml build",
    "docker-push": "docker compose -f .cloud/docker/docker-compose.prod.yml push",
    "copy-lock-file": "cp ../../yarn.lock yarn.lock",
    "taze": "taze major -w"
  },
  "engines": {
    "node": ">=22.11.0",
    "yarn": ">=1.22.22"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.7",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "apollo-server-core": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "graphql-type-json": "^0.3.2",
    "hbs": "^4.2.0",
    "helmet": "^8.0.0",
    "mysql2": "^3.11.4",
    "nest-commander": "^3.15.0",
    "nest-winston": "^1.9.7",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.16.0",
    "winston-discord-transport": "^1.3.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.6.0",
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.7",
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.9.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "taze": "^0.18.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn fix"
    ]
  },
  "workspaces": {
    "nohoist": [
      "**"
    ]
  }
}
